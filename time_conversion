#Time Conversions

def timeConversion(s):
    if "AM" in s and s[:2] == "12" in s:
        time = "00" + s[2:8]
        return time
    elif int(s[:2]) < 12 and "PM" in s:
        time = str(12 + int(s[:2])) + s[2:8]
        return time
    else:
        time = s[:8]
        return time
    
s = input('Enter a time: ')
my_time = timeConversion(s)
print(my_time)